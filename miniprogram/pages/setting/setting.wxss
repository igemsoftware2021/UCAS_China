page {
    background: #dbdadaef;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
  }
  .wrap{
      width:100%;
      padding:0;
      box-sizing:border-box;
  }

  .main-title{
      font-size:50rpx;
      font-weight:400;
      color:rgba(62,62,62,1);
      display:flex;
      justify-content: center;
      align-items: center;
  }
  .ticket-container{
    display:flex;
    flex-direction: row;
  }
  .title1{
    flex:auto;
  }
  .title2{
    flex:auto;
  }
  .swiper {
    width: 90%;
    flex: 1;
    height:1000rpx;
  }
  .name{
    padding:30rpx;
    font-size:34rpx;
    font-weight:400;
    color:rgba(62,62,62,1);
    display:flex;
    flex-direction:row;
  }
  .name.left{
    flex:auto;
  }
  .name.right{
    flex:auto;
  }

  .gender{
    padding:30rpx;
    font-size:34rpx;
    font-weight:400;
