/* pages/health/health.wxss */
page {
  background: #dbdadaef;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.navigator-group{
  height: 70vh;
  width: 100vw;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.naviagte-item{
  background-color: white;
  flex:1 1 auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 3px;
  margin-right: 3px;
  margin-top: 4px;
  margin-bottom: 4px;
  border-top-width: 2px;
  border-top-color: rgba(245, 235, 181, 0.281);
  border-bottom-width: 2px;
  border-bottom-color: rgba(245, 235, 181, 0.281);
  border-radius: 20px;
}
.navigator-part{
  height: 100%;
  padding-left: 20px;
  display: flex;
  flex-direction: row;
  justify-content: left;
}
.navigator-hover{
  background-color: rgb(191, 243, 3);
}
.icon{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.icon-img{
  width: 22px;
  height: 22px;
  margin-right: 5px;
}
.word-container{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center; 
  text-align: centerï¼›
}
.navigator-text{
  color:rgb(138, 99, 62);
  flex:0 0 auto;
  margin: auto;
}