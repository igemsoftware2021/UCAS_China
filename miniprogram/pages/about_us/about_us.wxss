/**index.wxss**/

page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.figure-swiper{
  width: 100vw;
  height: 26vh;
}
.image{
  height: 26vh;
}
.competition-name-text,.team-name-text{
  font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
  font-weight: bold;
  font-size: larger;
  color: darkslategray;
}
.competition-introduction-text,.team-introduction-text{
  font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;
  font-size: small;
}
.versions image{
  position: relative;
  width: 110px;
  height: 110px;
  display: flex;
  margin-top: 100rpx;
  justify-content: center;
}
.app-name{
  text-align: center;
  font: bolder;
  font-size: medium;
  font-weight: bolder;
}
.version {
  align-items: center;
  font: italic;
  display: flex;
  font-weight: lighter;
  font-size: smaller;
  position: relative;
  text-indent: 40px;
}
.bottom{
  bottom: 0px;
  text-align: center;
  display: flex;
  flex-direction: column;
  font: italic;
  font-weight: lighter;
  font-size:xx-small;
  position: fixed;
}
.privacy{
  color:cadetblue;
  text-decoration: underline;
}