/* pages/help/help_detail1.wxss */
page {
  background: #ecebebef;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;

} 
.title {
  width: 100%;
  height: 88rpx;
  background: rgb(234, 238, 234);
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.image{
  margin-left: 16px;
  width: 100%;
  height: 200px;
  margin: auto 0;
  margin-top: 10px;
  margin-bottom: 10px;
}
.article-content{
  color: #666;
  font-size: 28rpx;
  margin-bottom: 20rpx;
  margin-left: 30rpx;
  line-height: 40rpx;
  letter-spacing: 2rpx;
}
.article-images{
  width: 16px;
  height: 16px;
  margin-right: 8px;
  vertical-align: middle;
}
.article-text{
  vertical-align: middle;
}